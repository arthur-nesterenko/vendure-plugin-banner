{
    "name": "vendure-plugin-banner",
    "version": "1.0.0",
    "description": "Plugin template",
    "author": "Arthur Nestererenko <arthur.nesterenko.dev@gmail.com>",
    "repository": "https://github.com/vendure-ecommerce/plugin-template",
    "license": "MIT",
    "private": false,
    "engines": {
        "node": ">=18.0.0"
    },
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "files": [
        "dist",
        "README.md"
    ],
    "scripts": {
        "run:server": "ts-node ./dev-server/index.ts",
        "run:worker": "ts-node ./dev-server/index-worker.ts",
        "watch": "ts-node-dev  ./dev-server/index.ts",
        "start": "concurrently yarn:run:*",
        "build": "rimraf lib && tsc && gulp build",
        "test": "jest --preset=\"ts-jest\"",
        "generate-types": "graphql-codegen --config ./codegen.ts",
        "prepare": "husky",
        "lint": "eslint .",
        "prettify": "yarn prettier --write . --html-whitespace-sensitivity ignore",
        "lint:fix": "yarn lint --fix",
        "commit": "git-cz"
    },
    "devDependencies": {
        "@commitlint/cli": "^19.3.0",
        "@commitlint/config-conventional": "^19.2.2",
        "@commitlint/cz-commitlint": "^19.2.0",
        "@eslint/js": "^9.5.0",
        "@graphql-codegen/cli": "^5.0.2",
        "@graphql-codegen/typescript": "^4.0.7",
        "@graphql-codegen/typescript-document-nodes": "^4.0.7",
        "@graphql-codegen/typescript-operations": "^4.2.1",
        "@types/eslint__js": "^8.42.3",
        "@types/gulp": "^4.0.17",
        "@types/jest": "29.5.12",
        "@vendure/admin-ui-plugin": "2.2.5",
        "@vendure/asset-server-plugin": "2.2.5",
        "@vendure/core": "^2.2.5",
        "@vendure/testing": "2.2.5",
        "@vendure/ui-devkit": "^2.2.5",
        "better-sqlite3": "^11.0.0",
        "commitizen": "^4.3.0",
        "concurrently": "^8.2.2",
        "eslint": "^9.5.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-import": "^2.29.1",
        "eslint-plugin-prettier": "^5.1.3",
        "graphql-tag": "^2.12.6",
        "gulp": "^5.0.0",
        "husky": "^9.0.11",
        "inquirer": "9",
        "jest": "29.7.0",
        "rimraf": "^5.0.7",
        "ts-jest": "29.1.4",
        "ts-node": "10.9.2",
        "ts-node-dev": "^2.0.0",
        "typescript": "^5.4.5",
        "typescript-eslint": "^7.13.0"
    },
    "dependencies": {
        "sharp": "^0.33.4"
    }
}
